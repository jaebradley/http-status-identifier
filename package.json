{
  "name": "http-status-identifier",
  "version": "0.0.0-development",
  "description": "Get definitions for HTTP status codes",
  "files": [
    "build"
  ],
  "browser": "build/index.js",
  "main": "build/index.cjs.js",
  "module": "build/index.esm.js",
  "global": false,
  "engines": {
    "node": "10.14.1",
    "npm": "6.4.1"
  },
  "scripts": {
    "build": "BABEL_ENV=production rollup -c",
    "codecov": "codecov",
    "commitlint": "commitlint -e $GIT_PARAMS",
    "deploy": "npm run travis-deploy-once 'npm run semantic-release'",
    "gc": "commit",
    "is-es5": "es-check es5 build/index.js",
    "lint": "eslint --ext .js .",
    "prepare": "npm run build",
    "semantic-release": "semantic-release",
    "test": "BABEL_ENV=test jest src/ --coverage --no-cache",
    "travis-deploy-once": "travis-deploy-once"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jaebradley/http-status-identifier.git"
  },
  "keywords": [
    "http",
    "http status",
    "http status code",
    "http code"
  ],
  "author": "jae.b.bradley@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jaebradley/http-status-identifier/issues"
  },
  "homepage": "https://github.com/jaebradley/http-status-identifier#readme",
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "<rootDir>/build/",
      "<rootDir>/node_modules/"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/plugin-transform-async-to-generator": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/runtime": "^7.4.4",
    "@commitlint/cli": "^7.6.1",
    "@commitlint/config-angular": "^7.1.2",
    "@commitlint/prompt": "^7.6.1",
    "@commitlint/prompt-cli": "^7.6.1",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^24.8.0",
    "babel-preset-minify": "^0.5.0",
    "codecov": "^3.5.0",
    "dotenv": "^8.0.0",
    "es-check": "^5.0.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.2",
    "husky": "^2.3.0",
    "jest": "^24.8.0",
    "rollup": "^1.12.1",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-babel-minify": "^8.0.0",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-filesize": "^6.0.1",
    "rollup-plugin-local-resolve": "^1.0.7",
    "rollup-plugin-node-resolve": "^5.0.0",
    "rollup-plugin-terser": "^4.0.4",
    "semantic-release": "^15.13.14",
    "travis-deploy-once": "^5.0.9"
  },
  "peerDependencies": {}
}
